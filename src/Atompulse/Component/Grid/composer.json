{
  "name": "atompulse/grid",
  "description": "Atompulse Data Grid Component",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Petru Cojocar",
      "email": "petru.cojocar@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Atompulse\\Component\\Grid\\": ""
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/atompulse/PropelBundle",
      "no-api": true
    }
  ],
  "require": {
    "atompulse/domain": "dev-master",
    "propel/propel-bundle": "dev-build-properties-fix"
  },
  "require-dev": {
    "symfony/yaml": "^3.0"
  },
  "suggest": {
      "sokil/php-mongo": "Allows mongodb datasource"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
