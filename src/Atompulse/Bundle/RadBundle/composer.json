{
  "name": "atompulse/rad-bundle",
  "description": "Atompulse RAD Bundle",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Petru Cojocar",
      "email": "petru.cojocar@gmail.com"
    }
  ],
  "autoload": {
      "psr-4": { "Atompulse\\Bundle\\RadBundle\\": "" }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/Volumes/etc/workspace/atompulse/src/Atompulse/Component/FusionInclude"
    },
    {
      "type": "path",
      "url": "/Volumes/etc/workspace/atompulse/src/Atompulse/Bundle/FusionIncludeBundle"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/http-kernel": "^4",
    "symfony/http-foundation": "^4",
    "symfony/config": "^4",
    "symfony/dependency-injection": "^4",
    "friendsofsymfony/jsrouting-bundle": "2.0.*@dev"
  },
  "config": {
    "sort-packages": true,
    "vendor-dir": "vendor",
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
